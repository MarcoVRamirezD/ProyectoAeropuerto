<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="700.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cr.ac.ucenfotec.tl.usuario.PerfilUsuarioController">
    <children>
        <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#salir" prefHeight="40.0" prefWidth="100.0" style="-fx-background-color: #14213d;" text="Salir" textFill="WHITE">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Button>
      <Label alignment="CENTER" layoutX="240.0" layoutY="14.0" prefHeight="60.0" prefWidth="800.0" text="Bienvenido">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Button layoutX="415.0" layoutY="150.0" mnemonicParsing="false" onAction="#comprarTiquete" prefHeight="200.0" prefWidth="450.0" style="-fx-background-color: #14213d;" text="Ver tiquetes" textFill="WHITE">
         <font>
            <Font name="System Bold" size="28.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="415.0" layoutY="415.0" mnemonicParsing="false" onAction="#modificarPerfil" prefHeight="200.0" prefWidth="450.0" style="-fx-background-color: #14213d;" text="Modificar perfil" textFill="WHITE">
         <font>
            <Font name="System Bold" size="28.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
    </children>
</AnchorPane>
