<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="700.0" prefWidth="1280.0" style="-fx-border-color: #fca311; -fx-background-color: #14213d;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cr.ac.ucenfotec.tl.administrador.ListaUsuariosController">
    <children>
        <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#volver" prefHeight="40.0" prefWidth="100.0" style="-fx-background-color: #fca311;" text="Volver">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Button>
        <Button layoutX="415.0" layoutY="98.0" mnemonicParsing="false" onAction="#cargarListaUsuariosFinales" prefHeight="100.0" prefWidth="450.0" style="-fx-background-color: #fca311;" text="Mostrar">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
            <font>
                <Font name="System Bold" size="36.0" />
            </font>
        </Button>
        <TableView fx:id="tblUsuariosFinales" layoutX="45.0" layoutY="227.0" prefHeight="451.0" prefWidth="1190.0">
            <columns>
                <TableColumn fx:id="colUno" prefWidth="102.0" text="Nombre" />
                <TableColumn fx:id="colDos" prefWidth="82.0" text="Apellidos" />
                <TableColumn fx:id="colTres" prefWidth="127.0" text="Identificación" />
                <TableColumn fx:id="colCuatro" prefWidth="215.0" text="Correo" />
                <TableColumn fx:id="colCinco" minWidth="0.0" prefWidth="203.0" text="Dirección" />
                <TableColumn fx:id="colSeis" prefWidth="349.0" text="Nacionalidad" />
                <TableColumn fx:id="colSiete" prefWidth="110.0" text="Edad" />
            </columns>
        </TableView>
    </children>
</AnchorPane>
