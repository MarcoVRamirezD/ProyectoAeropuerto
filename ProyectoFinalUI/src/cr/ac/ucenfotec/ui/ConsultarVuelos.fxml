<AnchorPane prefHeight="700.0" prefWidth="1280.0" style="-fx-background-color: #14213D; -fx-border-color: #FCA311;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cr.ac.ucenfotec.tl.ConsultarVuelosController">
